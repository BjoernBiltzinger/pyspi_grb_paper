@article{Beckmann-2004,
title = {Time resolved spectroscopy of GRB030501 using INTEGRAL},
journal = {Nucl. Phys. B, Proc. Suppl.},
volume = {132},
pages = {301-304},
year = {2004},
note = {Proceedings of the 2nd BeppoSAX Conference: The Restless High-Energy Universe},
issn = {0920-5632},
doi = {https://doi.org/10.1016/j.nuclphysbps.2004.04.052},
url = {https://www.sciencedirect.com/science/article/pii/S0920563204001094},
author = {V. Beckmann and J. Borkowski and T.J.-L. Courvoisier and D. Götz and P. Favre and R. Hudec and S. Mereghetti and S.E. Shaw and A. {von Kienlin} and C. Wigger},
abstract = {The Gamma-ray instruments on-board INTEGRAL offer a unique opportunity to perform time resolved analysis on GRBs. The imager IBIS allows accurate positioning of GRBs and broad band spectral analysis, while SPI provides high resolution spectroscopy. GRB 030501 was discovered by the INTEGRAL Burst Alert System in the ISGRI field of view. Although the burst was fairly weak (fluence F20–200keV≃3.5×10−6 erg cm−2) it was possible to perform time resolved spectroscopy with a resolution of a few seconds. The GRB shows a spectrum in the 20 - 400 keV range which is consistent with a spectral photon index Γ=−1.8. No emission line or spectral break was detectable in the spectrum. Although the flux seems to be correlated with the hardness of the GRB spectrum, there is no clear soft to hard evolution seen over the duration of the burst.}
}
